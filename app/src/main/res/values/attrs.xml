<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputItemView">
        <!--左边标题-->
        <attr name="itemName" format="string" />
        <!--输入框提示-->
        <attr name="hintText" format="string" />
        <!--是否显示最右侧的获取验证码-->
        <attr name="isShowMoreCode" format="boolean" />
        <!--最长长度-->
        <attr name="maxLines" format="integer" />
        <!--输入类型-->
        <attr name="inputType" format="string"/>
        <!--左图资源图标-->
        <attr name="leftImage" format="reference"/>
        <!--是否可以编辑-->
        <attr name="canEdit" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="MineItemLayout">
        <attr name="has_mine_next" format="boolean"/>
        <attr name="has_mine_right_text" format="boolean"/>
        <attr name="has_mine_left_icon" format="boolean"/>
        <attr name="has_bottom_line" format="boolean"/>
        <attr name="left_mine_text" format="string" />
        <attr name="right_mine_text" format="string" />
        <attr name="left_mine_background" format="reference|color" />
    </declare-styleable>


    <declare-styleable name="CirclePercentView" >
        <attr name="type" format="integer"/>
        <attr name="circleBg" format="color"/>
        <attr name="circleBgTwo" format="color"/>
        <attr name="arcColorOne" format="color"/>
        <attr name="arcColorTwo" format="color"/>
        <attr name="arcColorThree" format="color"/>
        <attr name="arcColorFour" format="color"/>
        <attr name="arcWidth" format="dimension"/>
        <attr name="percentTextColor" format="color"/>
        <attr name="percentTextSize" format="dimension"/>
        <attr name="radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomCircleProgressBar">
        <attr name="outside_color" format="color" />
        <attr name="outside_radius" format="dimension" />
        <attr name="inside_color" format="color" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_width" format="dimension" />
        <attr name="max_progress" format="integer" />
        <attr name="progress" format="float" />
        <attr name="direction">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>


    <declare-styleable name="ClockView">
        <attr name="OneCircleColor" format="color"/>
        <attr name="TwoCircleColor" format="color"/>
        <attr name="animationTime" format="integer"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="hasText" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="CircleZhuView">
        <attr name="top_color" format="color" />
        <attr name="top_line_color_one" format="color" />
        <attr name="top_line_color_two" format="color" />
        <attr name="bottom_color" format="color" />
        <attr name="bottom_line_color_one" format="color" />
        <attr name="bottom_line_color_two" format="color" />
    </declare-styleable>
</resources>